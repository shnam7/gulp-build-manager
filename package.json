{
  "name": "gulp-build-manager",
  "version": "1.1.3",
  "description": "Modular gulp build manager using babel/es6. Gulp task configuration and made easy",
  "files": [
    "lib",
    "example",
    "README.md"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir lib",
    "watch-build": "npm run build -- --watch",
    "watch": "npm run watch-compile",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shnam7/gulp-build-manager.git"
  },
  "keywords": [
    "gulp",
    "build",
    "babel",
    "es6"
  ],
  "author": "Robin Soo-hyuk Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shnam7/gulp-build-manager/issues"
  },
  "homepage": "https://github.com/shnam7/gulp-build-manager#readme",
  "dependencies": {
    "del": "^2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-changed": "^2.0.0",
    "gulp-plumber": "^1.1.0",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-env": "^1.3.3",
    "babel-preset-latest": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-babel": "^6.1.2",
    "gulp-cleanhtml": "^1.0.3",
    "gulp-clone": "^1.0.0",
    "gulp-coffee": "^2.3.4",
    "gulp-concat": "^2.6.1",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^3.1.0",
    "gulp-html-prettify": "0.0.1",
    "gulp-imagemin": "^3.2.0",
    "gulp-livereload": "^3.8.1",
    "gulp-markdown": "^1.2.0",
    "gulp-nop": "0.0.3",
    "gulp-postcss": "^6.4.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.5.1",
    "gulp-twig": "^0.7.0",
    "gulp-typescript": "^3.1.6",
    "gulp-uglify": "^2.1.2",
    "gulp-zip": "^4.0.0",
    "merge-stream": "^1.0.1",
    "panini": "^1.4.0",
    "should": "^11.2.1",
    "typescript": "^2.2.2"
  }
}
