{
  "name": "gulp-build-manager",
  "version": "1.6.1",
  "description": "Gulp build manager using babel/es6. Gulp task made easy with configuration support",
  "files": [
    "lib",
    "example",
    "bin",
    "templates",
    "README.md"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir lib",
    "watch-build": "npm run build -- --watch",
    "prepublish": "npm run build",
    "test": "mocha --compilers js:babel-core/register --reporter spec"
  },
  "bin": {
    "gbm": "./bin/gbm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shnam7/gulp-build-manager.git"
  },
  "keywords": [
    "gulp",
    "build",
    "builder",
    "babel",
    "es6",
    "sass",
    "scss",
    "postcss",
    "twig",
    "coffeescript",
    "typescript",
    "markdown",
    "panini",
    "webpack"
  ],
  "author": "Robin Soo-hyuk Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shnam7/gulp-build-manager/issues"
  },
  "homepage": "https://github.com/shnam7/gulp-build-manager#readme",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-preset-latest": "^6.24.1",
    "commander": "^2.9.0",
    "del": "^2.2.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-livereload": "^3.8.1",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "gulp-save": "^1.2.1",
    "gulp-sourcemaps": "^2.6.0",
    "upath": "^1.0.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "should": "^11.2.1"
  }
}
