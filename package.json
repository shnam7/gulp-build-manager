{
  "name": "gulp-build-manager",
  "version": "2.0.1",
  "description": "Easy to use, flexible gulp task manager",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "rimraf lib && babel src --presets env --out-dir lib",
    "build-watch": "npm run build -- --watch",
    "build-docs": "gulp --cwd docs",
    "clean": "rimraf lib",
    "clean-docs": "rimraf _gh_pages docs/.jekyll-metadata",
    "test": "mocha --compilers js:babel-core/register --reporter spec",
    "build-all": "npm run build && npm run build-docs",
    "clean-all": "npm run clean && npm run clean-docs",
    "prepare": "babel src --presets env --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shnam7/gulp-build-manager"
  },
  "keywords": [
    "gulp",
    "build",
    "builder",
    "babel",
    "es6",
    "sass",
    "scss",
    "postcss",
    "twig",
    "coffeescript",
    "typescript",
    "markdown",
    "panini",
    "webpack"
  ],
  "author": "Robin Soo-hyuk Nam",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shnam7/gulp-build-manager/issues"
  },
  "homepage": "https://github.com/shnam7/gulp-build-manager#readme",
  "peerDependencies": {
    "gulp": ">=4.0"
  },
  "dependencies": {
    "del": "^3.0.0",
    "glob": "^7.1.2",
    "lodash.merge": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "upath": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "child_process": "^1.0.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-babel": "^7.0.0",
    "gulp-coffee": "^3.0.2",
    "gulp-coffeelint": "^0.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-debug": "^3.2.0",
    "gulp-eslint": "^4.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-html-prettify": "0.0.1",
    "gulp-htmlclean": "^2.7.16",
    "gulp-imagemin": "^4.1.0",
    "gulp-less": "^3.4.0",
    "gulp-livereload": "^3.8.1",
    "gulp-markdown": "^2.0.1",
    "gulp-order": "^1.1.1",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-twig": "^1.1.1",
    "gulp-typescript": "^3.2.3",
    "gulp-uglify": "^3.0.0",
    "gulp-zip": "^4.1.0",
    "lost": "^8.2.0",
    "panini": "^1.5.1",
    "postcss-cssnext": "^3.0.2",
    "postcss-inline-svg": "^3.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-utilities": "^0.7.0",
    "stylelint": "^8.4.0",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "tslint-loader": "^3.5.3",
    "twig-markdown": "^0.1.2",
    "typescript": "^2.6.2",
    "webpack-stream": "^4.0.0"
  }
}
